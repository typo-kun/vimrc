set nocompatible
set number
colorscheme desert
set tabstop=4
set softtabstop=4
set shiftwidth=4
set laststatus=2
set list
set listchars=tab:^\ ,eol:\|
set statusline=%<%f\ %m%r%h%w%{'['.(&fenc!=''?&fenc:&enc).']['.&ff.']'}%=%l,%c%V%8P
set wrap
set hlsearch
set scrolloff=10
set incsearch
set cindent
set showmatch
set ignorecase
syntax on
set tags=.tags
set wildmenu
